<form role="form" role="form" id="employerInfoForm">

<div class="row">

  <div class="col-md-offset-3 col-md-6 form-group">
      <label>Maker Name</label>
      <input type="text" value="{{model.name}}" placeholder=".col-md-6" class="form-control">
  </div>

</div>

<div class="row">

  <div class="col-md-offset-3 col-md-3 form-group">
      <label>Web Site</label>
      <input type="text" data-model-key="website" value="{{model.website}}" placeholder="Website" class="form-control">
  </div>


  <div class="col-md-3 form-group">
      <label>Farm</label>
      <input type="text" data-model-key="farm" value="{{model.farm}}" placeholder="Farm Name" class="form-control">
  </div>

</div>

<div class="row">

  <div class="col-md-offset-3 col-md-2 form-group">
      <label>Phone</label>
      <input type="text" data-model-key="phone" value="{{model.phone}}" placeholder="Phone Number" class="form-control">
  </div>

  <div class="col-md-2 form-group">
      <label>Contact Person</label>
      <input type="text" data-model-key="contact_person" value="{{model.contact_person}}" placeholder="Contact Person" class="form-control">
  </div>

  <div class=" col-md-2 form-group">
      <label>Hours</label>
      <input type="text" data-model-key="hours" value="{{model.hours}}" placeholder="Hours" class="form-control">
  </div>

</div>

<div class="row">
  <hr>
</div>

<div class="row">

  <div class="col-md-offset-3 col-md-3 form-group">
      <label>Twitter</label>
      <input type="text" data-model-key="twitter" value="{{model.twitter}}" placeholder="Twitter Screenname" class="form-control">
  </div>

  <div class="col-md-3 form-group">
      <label>Facebook</label>
      <input type="text" data-model-key="facebook" value="{{model.facebook}}" placeholder="Facebook Screenname" class="form-control">
  </div>

</div>

<div class="row">
  <hr>
</div>

<div class="row">

  <div class="col-md-offset-3 col-md-6 form-group">
      <label>Address</label>
      <input type="text" data-model-key="address" value="{{model.address}}" placeholder="Address" class="form-control">
  </div>

</div>

<div class="row">

  <div class="col-md-offset-3 col-md-2 form-group">
      <label>City</label>
      <input type="text" data-model-key="city" value="{{model.city}}" placeholder="City/Town Name" class="form-control">
  </div>

  <div class="col-md-2 form-group">
      <label>State</label>
      <input type="text" data-model-key="state" value="{{model.state}}" placeholder="State/Province" Name" class="form-control">
  </div>

  <div class="col-md-2 form-group">
      <label>Zip/Postal</label>
      <input type="text" data-model-key="zipcode" value="{{model.zipcode}}" placeholder="Zip/Postal Code" Name" class="form-control">
  </div>

</div>

<div class="row">

  <div class="col-md-offset-3 col-md-6 form-group">
      <label>Country</label>
      <input type="text" data-model-key="country" value="{{model.country}}" placeholder="Country" class="form-control">
  </div>

</div>

<div class="row">




</div>


<div class="row">

  <div class="form-group">
    <div class="col-lg-offset-8 col-lg-12">
      <button class="btn btn-icon btn-default glyphicons circle_ok saveButton">Save Changes</button>
      <button class="btn btn-icon btn-default glyphicons circle_ok cancelButton">Cancel</button>
    </div>
  </div>

</div>

</form>
